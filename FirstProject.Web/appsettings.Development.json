{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "ServiceUrls": {
    "AuthAPI": "https://localhost:5001",
    "ArticlesAPI": "https://localhost:5033"
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/comments?articleId={articleId}&index={index}&count={count}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 10009
        }
      ],
      "UpstreamPathTemplate": "/comments?articleId={articleId}&index={index}&count={count}",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/api/comments/getCount?articleId={articleId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 10009
        }
      ],
      "UpstreamPathTemplate": "/comments/getCount?articleId={articleId}",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/api/comments",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 10009
        }
      ],
      "UpstreamPathTemplate": "/comments",
      "UpstreamHttpMethod": [ "POST" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "oidc"
      },
      "DelegatingHandlers": [
        "HttpDelegatingHandler"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/comments/like",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 10009
        }
      ],
      "UpstreamPathTemplate": "/comments/like",
      "UpstreamHttpMethod": [ "PUT" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "oidc"
      },
      "DelegatingHandlers": [
        "HttpDelegatingHandler"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/comments/dislike",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 10009
        }
      ],
      "UpstreamPathTemplate": "/comments/dislike",
      "UpstreamHttpMethod": [ "PUT" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "oidc"
      },
      "DelegatingHandlers": [
        "HttpDelegatingHandler"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/comments/changecontent",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 10009
        }
      ],
      "UpstreamPathTemplate": "/comments/changecontent",
      "UpstreamHttpMethod": [ "PUT" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "oidc"
      },
      "DelegatingHandlers": [
        "HttpDelegatingHandler"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/comments?id={id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 10009
        }
      ],
      "UpstreamPathTemplate": "/comments?id={id}",
      "UpstreamHttpMethod": [ "DELETE" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "oidc"
      },
      "DelegatingHandlers": [
        "HttpDelegatingHandler"
      ]
    }
  ]
}
